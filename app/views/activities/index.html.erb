<div>
  <% @activities.each do |activity| %>
  <br>
    <div class="">
      <% activity.photos.each do |photo| %>
        <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
      <% end %>
      <%= link_to "Show this activity", activity_path(activity), class: "btn btn-primary" %>
      <%= render activity %>
    </div>
  <% end %>
</div>
